{
  "compilerOptions": {
    "allowJs": true,
    "target": "ES5",
    "module": "Node16",
    "outDir": "build",
    "rootDir": "src",
    "allowSyntheticDefaultImports": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "paths": {
      "@Types/*": ["./src/Types/*"]
    }
  },
  "include": ["src/**/*.ts", "environment.d.ts"],
  "exclude": ["node_modules"]
}
